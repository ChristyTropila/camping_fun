<h1> Add a New Camper:</h1>
<br>
<%= form_for @camper do |f| %>
    <h3><%= f.label :Name %></h3>
    <%= f.text_field :name %> <br/>
    <br>
    <h3><%= f.label :Age %></h3>
    <%= f.number_field :age %><br/>
    <br>
    <br>
    <%= f.submit "Add Camper" %>
<% end %>
<br>
<%= button_to("Back to All Campers", campers_path, method: :get) %><br>
